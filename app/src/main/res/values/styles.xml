<?xml version="1.0" encoding="utf-8"?>
<resources>
        <style name="Travelling.Icon.Button" parent="@style/Widget.MaterialComponents.Button.OutlinedButton">
            <item name="android:layout_width">80dp</item>
            <item name="android:layout_height">70dp</item>
            <item name="cornerRadius">10dp</item>
            <item name="iconGravity">textStart</item>
            <item name="iconPadding">-1dp</item>
            <item name="iconSize">40dp</item>
            <item name="iconTint">@color/black</item>
        </style>

        <style name="Travelling.Home.Card" parent="@style/CardView">
            <item name="android:layout_width">match_parent</item>
            <item name="android:layout_height">270dp</item>
            <item name="android:layout_marginHorizontal">8dp</item>
            <item name="cardCornerRadius">12dp</item>
            <item name="cardElevation">10dp</item>
        </style>
    <style name="RoundedBottom">
        <item name="cornerSizeBottomLeft">30dp</item>
        <item name="cornerSizeBottomRight">30dp</item>
    </style>

    <style name="btn_test1">
        <item name="android:textColor">@color/rosa</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:backgroundTintMode">screen</item>
    </style>
</resources>